@{
    Layout = null;
}
<div ng-show="data.isLoad()">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Имя</th>
                <th>Фамилия</th>
                <th>Телефон</th>
                <th>E-mail</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="item in data.users.items">
                <td>{{item.name}}</td>
                <td>{{item.lastName}}</td>
                <td>{{item.phone}}</td>
                <td>{{item.email}}</td>
            </tr>
        </tbody>
    </table>
</div>
